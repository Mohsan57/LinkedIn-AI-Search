{
  "manifest_version": 3,
  "name": "LinkedIn Resume Matcher",
  "version": "2.0",
  "description": "Generate and compare resumes for LinkedIn job listings.",
  "permissions": ["activeTab", "storage","nativeMessaging", "scripting","tabs","sidePanel","http://*/*", "https://*/*"],
  "side_panel": {
    "default_path": "HtmlForms/popup.html",
    "use_frames": true
  },
  "background": {
    "service_worker": "Scripts/background.js"
  },

  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [{
    "js": ["libraries/jquery.min.js", "Scripts/content.js", "Scripts/contentScript.js"],
    "matches": ["https://www.linkedin.com/jobs/*"]
  }]
  
}

